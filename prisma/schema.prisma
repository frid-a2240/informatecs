model authStudents {
  id             Int      @id @default(autoincrement())
  matricula      String   @unique
  password       String
  nombreCompleto String
  correo         String?   // Correo que el usuario ingresa para verificaci칩n
  isVerified     Boolean   @default(false) // Se pone en true cuando valida el correo y cambia contrase침a
  emailCode      String?   // C칩digo temporal enviado al correo para validaci칩n
  createdAt      DateTime  @default(now())
  updatedAt      DateTime?  @updatedAt
}
